{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://lines.navalclash.com/schemas/oldsdb/jobs_resp.json",
  "type": "object",
  "title": "/oldsdb/jobs server response",
  "description": "Server response for /oldsdb/jobs request",
  "default": {},
  "examples": [
    {
      "success": true,
      "data": [
        {
          "id":  1,
          "jobid":  "123456",
          "workerid":  1,
          "client":  "MI-6",
          "description":  "Invisible suite for 007",
          "startdts":  "2018-11-13T20:20:39.000Z",
          "enddts":  "2018-11-13T20:20:39.000Z",
          "jobstatus":  "Finished",
          "jobtype": "New"
        }
      ]
    }
  ],
  "required": [
    "success",
    "data"
  ],
  "properties": {
    "success": {
      "$id": "#/properties/success",
      "type": "boolean",
      "title": "Operation success result",
      "description": "Indicates successful result or not (true/false)",
      "default": false,
      "examples": [
        true
      ]
    },
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "title": "Response result data",
      "description": "Result object containing server set data",
      "additionalProperties": false,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {
            "$id": "#/properties/data/id",
            "type": "integer",
            "title": "Inner job  id",
            "description": "Inner job  id",
            "default": false,
            "examples": [
              1
            ]
          },
          "jobid": {
            "$id": "#/properties/data/jobid",
            "type": "string",
            "title": "JobID",
            "description": "D4D ID",
            "default": false
          },
          "workerid": {
            "$id": "#/properties/data/workerid",
            "type": "integer",
            "title": "worker id",
            "description": "worker",
            "default": false
          },
          "client": {
            "$id": "#/properties/data/client",
            "type": "string",
            "title": "client",
            "description": "client name",
            "default": false
          },
          "description": {
            "$id": "#/properties/data/description",
            "type": "string",
            "title": "description",
            "description": "job description",
            "default": false
          },
          "startdts": {
            "$id": "#/properties/data/startdts",
            "type": "object",
            "format": "date-time",
            "title": "startdts",
            "description": "job start",
            "default": false
          },
          "enddts": {
            "$id": "#/properties/data/enddts",
            "type": "object",
            "format": "date-time",
            "title": "enddts",
            "description": "job end",
            "default": false
          },
          "jobstatus": {
            "$id": "#/properties/data/jobstatus",
            "type": "string",
            "title": "jobstatus",
            "description": "job status",
            "default": false,
            "enum": [
              "Active",
              "Finished",
              "Waiting"
            ]
          },
          "jobtype": {
            "$id": "#/properties/data/jobtype",
            "type": "string",
            "title": "jobtype",
            "description": "job type",
            "default": false,
            "enum": [
              "New",
              "Continued"
            ]
          }
        },
        "required": [
          "id",
          "jobid",
          "workerid",
          "client",
          "description",
          "startdts",
          "enddts",
          "jobstatus",
          "jobtype"
        ]
        }

    }
  }

}