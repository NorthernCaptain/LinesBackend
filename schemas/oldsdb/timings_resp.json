{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://lines.navalclash.com/schemas/oldsdb/timings_resp.json",
  "type": "object",
  "title": "/oldsdb/timings server response",
  "description": "Server response for /oldsdb/timings request",
  "default": {},
  "examples": [
    {
      "success": true,
      "data": [
        {
          "jobid":  "123456",
          "workerid":  1,
          "startdts":  "2018-11-13T20:20:39.000Z",
          "enddts":  "2018-11-13T20:20:39.000Z"
        }
      ]
    }
  ],
  "required": [
    "success",
    "data"
  ],
  "properties": {
    "success": {
      "$id": "#/properties/success",
      "type": "boolean",
      "title": "Operation success result",
      "description": "Indicates successful result or not (true/false)",
      "default": false,
      "examples": [
        true
      ]
    },
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "title": "Response result data",
      "description": "Result object containing server set data",
      "additionalProperties": false,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "jobid": {
            "$id": "#/properties/data/jobid",
            "type": "integer",
            "title": "JobID",
            "description": "D4D Job ID",
            "default": false
          },
          "workerid": {
            "$id": "#/properties/data/gemid",
            "type": "integer",
            "title": "worker id",
            "description": "worker",
            "default": false
          },
          "startdts": {
            "$id": "#/properties/data/startdts",
            "type": "object",
            "format": "date-time",
            "title": "startdts",
            "description": "start of interval",
            "default": false
          },
          "enddts": {
            "$id": "#/properties/data/enddts",
            "type": "object",
            "format": "date-time",
            "title": "enddts",
            "description": "end of interval",
            "default": false
          }
        },
        "required": [
          "jobid",
          "workerid",
          "startdts",
          "enddts"
        ]
        }

    }
  }

}