{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://us.navalclash.com/schemas/oldsdb/workers_resp.json",
  "type": "object",
  "title": "/oldsdb/workers server response",
  "description": "Server response for /oldsdb/workers request",
  "default": {},
  "examples": [
    {
      "success": true,
      "data": [
        {
        "id": 1,
        "name": "Uncle Sam",
        "description": "u_sam@gmail.com"
        }
      ]
    }
  ],
  "required": [
    "success",
    "data"
  ],
  "properties": {
    "success": {
      "$id": "#/properties/success",
      "type": "boolean",
      "title": "Operation success result",
      "description": "Indicates successful result or not (true/false)",
      "default": false,
      "examples": [
        true
      ]
    },
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "title": "Response result data",
      "description": "Result object containing server set data",
      "additionalProperties": false,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {
            "$id": "#/properties/data/id",
            "type": "integer",
            "title": "Worker id",
            "description": "Gem ID - PK integer",
            "default": false,
            "examples": [
              1
            ]
          },
          "name": {
            "$id": "#/properties/data/name",
            "type": "string",
            "title": "Full Name",
            "description": "Person's name",
            "default": false,
            "examples": [
              "Uncle Sam",
              "Santa Claus"
            ]
          },
          "description": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "description",
            "description": "Info",
            "default": false,
            "examples": [
              "s_clause@gmail.com",
              "973-200-222"
            ]
          }
        },
        "examples": [
          {
            "id": 1,
            "code": "Santa Claus",
            "name": "s_clause@gmail.com"
          }
        ],
        "required": [
          "id",
          "name",
          "description"
        ]
        }

    }
  }

}