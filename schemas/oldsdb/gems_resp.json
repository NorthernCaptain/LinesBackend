{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://lines.navalclash.com/schemas/oldsdb/gems_resp.json",
  "type": "object",
  "title": "/oldsdb/gems server response",
  "description": "Server response for /oldsdb/gems request",
  "default": {},
  "examples": [
    {
      "success": true,
      "data": [
        {
          "id":  1,
          "jobid":  "123456",
          "gemid":  1,
          "cnt":  10.5,
          "dts":  "2018-11-13T20:20:39.000Z"
        }
      ]
    }
  ],
  "required": [
    "success",
    "data"
  ],
  "properties": {
    "success": {
      "$id": "#/properties/success",
      "type": "boolean",
      "title": "Operation success result",
      "description": "Indicates successful result or not (true/false)",
      "default": false,
      "examples": [
        true
      ]
    },
    "data": {
      "$id": "#/properties/data",
      "type": "array",
      "title": "Response result data",
      "description": "Result object containing server set data",
      "additionalProperties": false,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {
            "$id": "#/properties/data/id",
            "type": "integer",
            "title": "gemuseid",
            "description": "Gem using  id",
            "default": false,
            "examples": [
              1
            ]
          },
          "jobid": {
            "$id": "#/properties/data/jobid",
            "type": "integer",
            "title": "JobID",
            "description": "D4D Job ID",
            "default": false
          },
          "gemid": {
            "$id": "#/properties/data/gemid",
            "type": "integer",
            "title": "gem id",
            "description": "id from gem_list",
            "default": false
          },
          "cnt": {
            "$id": "#/properties/data/cnt",
            "type": "integer",
            "title": "quantity",
            "description": "gem quantity",
            "default": false
          },
          "dts": {
            "$id": "#/properties/data/dts",
            "type": "object",
            "format": "date-time",
            "title": "dts",
            "description": "date of gem use",
            "default": false
          }
        },
        "required": [
          "id",
          "jobid",
          "gemid",
          "cnt",
          "dts"
        ]
        }

    }
  }

}